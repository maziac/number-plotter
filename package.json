{
    "name": "number-plotter",
    "displayName": "Number Plotter",
    "description": "Creates plots/charts from a series of selected numbers.",
    "version": "0.1.0",
    "publisher": "maziac",
    "license": "MIT",
    "keywords": [
        "number",
        "series",
        "plot",
        "plotter",
        "graph",
        "chart"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maziac/number-plotter.git"
    },
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "assets/icon.png",
    "activationEvents": [
        "workspaceContains:*"
    ],
    "main": "./out/extension",
    "scripts": {
        "compile": "tsc -p ./",
        "tslint": "eslint ./**/*.ts",
        "watch": "tsc -w -p ./",
        "test": "mocha -u tdd ./out/tests/",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "@types/node": "^13.13.39",
        "@types/vscode": "^1.52.0",
        "typescript": "^4.1.3"
    },
    "contributes": {
        "commands": [
            {
                "command": "number-plotter.plot",
                "title": "Plots the selected series of numbers",
                "category": "number-plotter"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "number-plotter.plot"
                }
            ],
            "editor/context": [
                {
                    "command": "number-plotter.plot"
                }
            ]
        }
    }
}
